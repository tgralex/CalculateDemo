{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "FormOptions": {
    "MultipartBodyLengthLimit": "100_000"
  },
  "IpRateLimitOptions": {
    "PerPeriod": "1s",
    "Limit": "1"
  },
  "AllowedHosts": "*"
}
